import kotlin.collections.List;

CREATE TABLE Schedule(
id TEXT NOT NULL PRIMARY KEY,
event_id TEXT NOT NULL,
time TEXT NOT NULL,
room TEXT NOT NULL,
title TEXT NOT NULL,
speakers TEXT AS List<String> NOT NULL
);

insertSchedule:
INSERT OR REPLACE INTO Schedule(
id, event_id, time, room, title, speakers
) VALUES (?,?,?,?, ?, ?);

selectScheduleItems:
SELECT * FROM Schedule WHERE event_id == ? ORDER BY time;
